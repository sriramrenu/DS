{
    "version": 2,
    "builds": [
        {
            "src": "backend/src/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/src/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ],
    "regions": [
        "pdx1"
    ]
}